<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator android:duration="1800">
        <propertyValuesHolder
            android:propertyName="rotation"
            android:valueFrom="0.0"
            android:valueTo="765.0"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="scaleX"
            android:valueFrom="0.1"
            android:valueTo="0.5"
            android:valueType="floatType" />
        <propertyValuesHolder
            android:propertyName="scaleY"
            android:valueFrom="0.1"
            android:valueTo="0.5"
            android:valueType="floatType" />
    </objectAnimator>

    <objectAnimator
        android:duration="400"
        android:propertyName="rotation"
        android:valueFrom="765.0"
        android:valueTo="685.0"
        android:valueType="floatType" />
    <objectAnimator
        android:duration="400"
        android:propertyName="rotation"
        android:valueFrom="685.0"
        android:valueTo="720.0"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="800"
        android:propertyName="alpha"
        android:valueFrom="1f"
        android:valueTo="1f"
        android:valueType="floatType" />

    <objectAnimator android:duration="400">
        <propertyValuesHolder
            android:propertyName="rotation"
            android:valueFrom="720.0"
            android:valueTo="765.0"
            android:valueType="floatType" />

        <propertyValuesHolder
            android:propertyName="scaleX"
            android:valueFrom="0.5"
            android:valueTo="0.6"
            android:valueType="floatType" />
        <propertyValuesHolder
            android:propertyName="scaleY"
            android:valueFrom="0.5"
            android:valueTo="0.6"
            android:valueType="floatType" />
    </objectAnimator>
    <objectAnimator
        android:duration="400"
        android:propertyName="rotation"
        android:valueFrom="765.0"
        android:valueTo="685.0"
        android:valueType="floatType" />
    <objectAnimator
        android:duration="400"
        android:propertyName="rotation"
        android:valueFrom="685.0"
        android:valueTo="720.0"
        android:valueType="floatType" />
    <objectAnimator android:duration="400">
        <propertyValuesHolder
            android:propertyName="scaleX"
            android:valueFrom="0.6"
            android:valueTo="0"
            android:valueType="floatType" />
        <propertyValuesHolder
            android:propertyName="scaleY"
            android:valueFrom="0.6"
            android:valueTo="0"
            android:valueType="floatType" />
    </objectAnimator>
</set>